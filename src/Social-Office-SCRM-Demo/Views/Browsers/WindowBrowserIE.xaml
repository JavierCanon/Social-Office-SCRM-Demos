<Window x:Class="SODemo.Views.Browsers.WindowBrowserIE"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SODemo.Views.Browsers"
        mc:Ignorable="d"
        Title="Browser IE" Height="600" Width="800" Loaded="Window_Loaded">
    <Grid MinWidth="800" MinHeight="600">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>            
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <WebBrowser Grid.Column="0" Grid.Row="0" x:Name="webBrowser" HorizontalAlignment="Left" Height="Auto" Width="Auto"  />

    </Grid>
</Window>
