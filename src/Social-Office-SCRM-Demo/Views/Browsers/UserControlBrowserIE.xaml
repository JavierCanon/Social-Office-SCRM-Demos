<UserControl x:Class="SODemo.Views.Browsers.UserControlBrowserIE"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SODemo.Views.Browsers"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <WebBrowser x:Name="browser" Grid.Row="0" Grid.Column="0"  HorizontalAlignment="Left" 
                   MinWidth="600"  MinHeight="480"
                   VerticalAlignment="Top" Loaded="Browser_Loaded"/>

    </Grid>
</UserControl>
